{
    "plugins": ["prettier"],
    "extends": [
      "plugin:prettier/recommended"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "parser": "flow"
        }
      ],
      "arrow-body-style": "off",
      "prefer-arrow-callback": "off"
    }
  }